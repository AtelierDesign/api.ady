{
  "name": "api.ady",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"node api-server\"",
    "dev:api": "nodemon api-server",
    "build": "next build",
    "start": "concurrently \"next start\" \"node api-server\"",
    "start:api": "node api-server",
    "test": "jest tests --coverage --maxWorkers=10",
    "test:watch": "jest --coverage --watch",
    "test:integration": "start-server-and-test start http-get://localhost:3000 cypress:run",
    "test:integration:watch": "start-server-and-test start http-get://localhost:3000 cypress:open",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^1.6.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@radix-ui/colors": "^0.1.7",
    "@radix-ui/react-aspect-ratio": "^0.1.1",
    "@radix-ui/react-icons": "^1.0.3",
    "@radix-ui/react-id": "^0.1.1",
    "@stitches/core": "^1.2.5",
    "@stitches/react": "^1.2.5",
    "auth0-lock": "^11.31.1",
    "bootstrap": "^5.1.3",
    "concurrently": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "helmet": "^4.3.1",
    "highlight.js": "^11.0.1",
    "jwks-rsa": "^2.0.3",
    "morgan": "^1.10.0",
    "next": "^11.0.1",
    "next-themes": "^0.0.15",
    "nodemon": "^2.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reactstrap": "^8.8.1",
    "sass": "^1.43.4",
    "stitches-mixins": "^1.0.0"
  },
  "devDependencies": {
    "@babel/parser": "^7.16.2",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@testing-library/dom": "^8.0.0",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^12.0.0",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^27.0.6",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "postcss": "^8.3.11",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.1",
    "prettier-eslint": "^13.0.0",
    "start-server-and-test": "^1.11.7"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "rootDir": "./",
    "moduleNameMapper": {
      ".(css)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "setupFiles": [
      "./tests/environment.js"
    ],
    "setupFilesAfterEnv": [
      "./tests/setup.js"
    ],
    "collectCoverageFrom": [
      "components/**/*.*",
      "pages/**/*.*"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "text-summary"
    ]
  },
  "description": "<link rel=\"stylesheet\" href=\"https://cdn.auth0.com/js/auth0-samples-theme/1.0/css/auth0-theme.min.css\" /> This sample demonstrates the integration of [Auth0 Next.js SDK](https://github.com/auth0/nextjs-auth0) into a Next.js application created using [create-next-app](https://nextjs.org/docs/api-reference/create-next-app). The sample is a companion to the [Auth0 Next.js SDK Quickstart](https://auth0.com/docs/quickstart/webapp/nextjs).",
  "main": "index.jsx",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AtelierDesign/api.ady.git"
  },
  "author": "chvndler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AtelierDesign/api.ady/issues"
  },
  "homepage": "https://github.com/AtelierDesign/api.ady#readme"
}
